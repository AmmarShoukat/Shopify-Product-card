jQuery('.nws-pdp-images').slick({
    dots: true,
    arrows: true,
    infinite: true,
    slidesToShow: 1,
    focusOnSelect: true,
    autoplay: false,
    slidesToScroll: 1,
}); 

//cart varient js
jQuery(".nws-variant-box").click(function(){
  var id = jQuery(this).attr("data-id");
  jQuery("#"+id+" .nws-variant-box").removeClass('nws-active');
  jQuery(this).addClass('nws-active');
});
